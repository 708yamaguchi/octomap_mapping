<launch>
  <include file="$(find octomap_server)/launch/play_rosbag.launch">
    <arg name="filename" value="$(find octomap_server)/rosbag/microwave.bag" />
    <arg name="rviz" value="false"/>
  </include>

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find octomap_server)/config/microwave_success.rviz"/>

  <node name="service_call_for_rosbag" pkg="octomap_server" type="service-call-for-rosbag-microwave.l" />
</launch>
